    "@angular/compiler-cli": "^6.0.9",
    "@angular/platform-server": "^6.0.9",
    "@types/mocha": "^2.2.43",
    "@types/node": "9.3.0",
    "chai": "^4.1.2",
    "coveralls": "2.13.1",
    "fetch-mock": "5.11.0",
    "growl": "1.10.5",
    "hoek": "4.2.1",
